{
  "manifest_version": 2,
  "name": "MontanoProject The Ripper",
  "version": "1.0",
  "permissions": [
    "http://localhost:3000/*",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.reddit.com/*"],
      "js": ["TheRipper.js"]
    }
  ]
}
